  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --progress",
    "start-build": "cross-env NODE_ENV=production webpack-dev-server --hot --inline --progress",
    "test": "jest --no-cache --verbose",
    "jest": "jest --watch",
    "build": "cross-env NODE_ENV=production webpack --progress --profile --colors",
    "stats": "cross-env NODE_ENV=production webpack --json | webpack-bundle-size-analyzer",
    "prelint:css": "cross-env npm run build",
    "lint:css": "immutable-css public/*.css --strict",
    "lint:js": "eslint --quiet **/*.js"
  },
  "license": "UNLICENSED",
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/src/.*/__mocks__"
    ],
    "moduleNameMapper": {
      "^lib(.*)$": "<rootDir>/src/lib$1",
      "^.+\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.(jpg|png|gif|svg)$": "<rootDir>/src/__mocks__/transform.js"
    }
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "preact": "^8.1.0"
  },
